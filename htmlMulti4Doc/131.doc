    //初始化图片
    function u_u5_init_img()
    {

            u_u5_imgs.addImg("\/__local\/A\/4A\/03\/8BC2AC65404FCAD7CC2348FDC50_6BB74B8B_3D07.jpg", "0", u_u5_onimgload);

