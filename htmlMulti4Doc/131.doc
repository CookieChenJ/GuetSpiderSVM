            u_u5_imgs.addImg("\/__local\/3\/B0\/FD\/7A4459DE5D6EEB1BF903045E1EF_799F29DC_11238.jpg", "2", u_u5_onimgload);

            u_u5_imgs.addImg("\/__local\/2\/5F\/FD\/EA360A7EFA80FEBBBAC55CB0BA0_38832503_CCE2.jpg", "3", u_u5_onimgload);

            u_u5_imgs.addImg("\/__local\/1\/50\/AA\/D88075A552D447552C328065B3B_43CB92AF_B653.jpg", "4", u_u5_onimgload);

    }
    function u_u5_onimgload(img, imgid)
    {
        var imgobjs = document.getElementsByName("u_u5_img");
        var imgindex = new Number(imgid);
        
        img.showImg(imgobjs[imgindex], 150, 112, true, true);
        img.showImg(imgobjs[imgindex+ 5], 150, 112, true, true);
    }

    var u_u5_MyMar;
    var u_u5_speed3;
    var u_u5_move = true;
    var u_u5_movenum = 0;
    var u_u5_pos = null; //td位置组
    
    function u_u5_initpos()
    {
        u_u5_pos = new Array();
        
        var tds1 = document.getElementById("u_u5_demo1").childNodes[0].rows[0].cells;
        
        for(i = 0 ;i < tds1.length; i++)
        {
            u_u5_pos[u_u5_pos.length] = tds1[i].offsetLeft;
        }
        
        var tds2 = document.getElementById("u_u5_demo2").childNodes[0].rows[0].cells;
        var tds2offset = document.getElementById("u_u5_demo2").offsetLeft;
        for(i = 0 ;i < tds2.length; i++)
        {
            u_u5_pos[u_u5_pos.length] = tds2[i].offsetLeft + tds2offset;
        }
    }
    function u_u5_getpos(scrollpos)
    {
        for(i = 0; i < u_u5_pos.length; i++)
        {
            if(scrollpos < u_u5_pos[i])
                return i-1;
        }
    }

    function u_u5_Marquee() 
    {
        try 
        { 
            if(!u_u5_move)
                return;
            
            var demoobj = document.getElementById("u_u5_demo");
            var demo1obj = document.getElementById("u_u5_demo1");
            //滚动步长
            var scrollstep = 5
            
            //当前位置
            var oldscrollLeft = demoobj.scrollLeft;
            
            //更新后的位置, 已考虑过回滚
            var newscrollLeft = (oldscrollLeft + scrollstep)%demo1obj.scrollWidth;
            
            demoobj.scrollLeft = newscrollLeft; 
            
        } 
        catch(e) 
        { 
        } 
    } 
    function u_u5_clearInterval()
    {
        clearInterval(u_u5_MyMar);
    }
    
    function u_u5_setInterval()
    {
        u_u5_MyMar = setInterval(u_u5_Marquee,u_u5_speed3);
    }
    
    function u_u5_stopmove()
    {
        u_u5_move = false;
    }
    function u_u5_startmove()
    {
        u_u5_move = true;
    }
    
    function u_u5_init()
    {
        u_u5_speed3 = 100;
        if(u_u5_speed3 < 1)
        {
            u_u5_speed3 = 1;
        }
        
        document.getElementById("u_u5_demo").onmouseover = u_u5_stopmove;
        document.getElementById("u_u5_demo").onmouseout = u_u5_startmove;
        document.getElementById("u_u5_demo2").innerHTML = document.getElementById("u_u5_demo1").innerHTML;
        
        u_u5_init_img();

        u_u5_setInterval();

    }
    
    u_u5_init();
